{
    "version": 2,
    "builds": [{
        "src": "server.js",
        "use": "@now/node-server"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET"]
    }],
    "env": {
        "CORSANYWHERE_WHITELIST_TARGETS": "www.optimade.org,optimade.org,optimade-index.odbx.science,odbx.science,nomad-lab.eu,www.materialscloud.org,materialscloud.org,providers.optimade.science,providers.optimade.org,www.crystallography.net,aiida-dev.materialscloud.org,dev-aiida-dev.materialscloud.org,optimade.materialsproject.org,repository.nomad-coe.eu,optimade.odbx.science"
    }
}
