{
    "version": 2,
    "builds": [{
        "src": "server.js",
        "use": "@now/node-server"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/server.js",
        "methods": ["GET"]
    }],
    "env": {
        "CORSANYWHERE_WHITELIST_TARGETS": "providers.optimade.science,providers.optimade.org,www.crystallography.net,aiida.materialscloud.org,optimade.materialsproject.org,api.mpds.io,optimade.odbx.science,optimade.openmaterialsdb.se,oqmd.org"
    }
}
